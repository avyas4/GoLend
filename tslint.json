{
  "defaultSeverity": "warn",
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "cyclomatic-complexity": [
      true,
      10
    ],
    "no-console": false,
    "no-submodule-imports": [
      false
    ],
    "comment-format": false,
    "file-name-casing": [
      true,
      "pascal-case"
    ]
  }
}